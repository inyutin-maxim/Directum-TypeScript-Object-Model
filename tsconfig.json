{
  "compilerOptions": {
    "module": "commonjs",
    "moduleResolution": "node",
    "removeComments": true,
    "preserveConstEnums": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noEmitOnError": true,
    "strictNullChecks": true,
    "target": "es5",
    "sourceMap": true,
    "outFile": "Cover/js/tsc.js",
    "outDir": "Cover/js/"
  },
  "include": [
    "Directum/**/*"
  ],
  "compileOnSave": true,
  "exclude": [
    "node_modules"
  ]
}